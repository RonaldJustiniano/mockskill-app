        <script><![CDATA[(function producerScript(producer) {
  // Encontra o registro "Colaborador" vinculado ao usuário atual
  var col = new GlideRecord('x_snc_mock_skillup_colaborador');
  col.addQuery('usuario', gs.getUserID());
  col.setLimit(1);
  col.query();
  if (!col.next()) {
    gs.addErrorMessage('Não foi possível localizar seu registro de Colaborador. Contate o suporte.');
    current.setAbortAction(true);
    return;
  }

  // Preenche o campo colaborador do novo registro
  current.colaborador = col.getUniqueValue();

  // (Opcional) se existir o campo "ativo" na intermediária, defina como true
  if (current.isValidField('ativo')) {
    current.ativo = true;
  }
})(producer);
]]></script>
