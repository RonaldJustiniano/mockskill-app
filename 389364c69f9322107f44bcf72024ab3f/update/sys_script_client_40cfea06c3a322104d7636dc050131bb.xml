        <messages/>
        <name>validar data inicio</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
  if (isLoading) return;

  var ini = g_form.getValue('u_data_inicio');
  var fim = g_form.getValue('u_data_fim');
  if (ini && fim) {
    var dIni = new Date(ini);
    var dFim = new Date(fim);

    if (dFim < dIni) {
      g_form.showFieldMsg('u_data_fim', 'A data de fim não pode ser anterior à data de início.', 'error');
    } else {
      g_form.hideFieldMsg('u_data_fim', true);
    }
  }
}
]]></script>
