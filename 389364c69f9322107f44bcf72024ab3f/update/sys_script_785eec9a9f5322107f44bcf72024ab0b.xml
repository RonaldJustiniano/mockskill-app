        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

  if (!current.colaborador || current.supervisor) return;

  var col = new GlideRecord('x_snc_mock_skillup_colaborador');
  if (!col.get(current.colaborador)) return;

  if (!col.departamento) return;

  var sup = new GlideRecord('x_snc_mock_skillup_supervisor');
  sup.addQuery('departamento', col.departamento);
  sup.setLimit(1);
  sup.query();
  if (sup.next()) current.supervisor = sup.getUniqueValue();

})(current, previous);]]></script>
